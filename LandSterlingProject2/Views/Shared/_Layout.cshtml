            </a>
            <button class="navbar-toggler"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent"
                    aria-expanded="false"
                    aria-label="Toggle navigation">
                <i class="Toggle fa-solid fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mb-2 mb-lg-0 lg-ps-5">
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Index">Buy</a>
                    </li>
                    <li class="nav-item pe-2">
                        <a class="nav-link" href="~/Home/SellYourProperty">Sell your property</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link d-flex align-items-center" href="#">
                            hot deals
                            <img class="ms-2 icon" src="~/images/fire.svg" alt="svg" />
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Properties">Properties</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/AboutUs">About Us</a>
                    </li>
                </ul>

                <div class="nav-icon d-flex lg-ps-4 lg-pe-4">
                    <li class="lg-ps-4 sm-p-2">
                        <a href="tel:+97145826135"> <img class="icon" src="~/images/call small.svg" alt="icon" /></a>
                    </li>
                    <li class="lg-ps-4 sm-p-2">
                        <a href="https://wa.me/+97145826135">  <img class="icon" src="~/images/whatsapp small.svg" alt="icon" /></a>
                    </li>
                    <li class="lg-ps-4 sm-p-2">
                        <a href="~/Home/ContactUs"> <img class="icon" src="~/images/mail small.svg" alt="icon" /></a>
                    </li>
                    @if ((@Html.Action("GetUserName", "User")).ToString() != "")
                    {
                        <li  class="lg-ps-4 sm-p-4">
                        <a href="~/Home/SavedProperties">
                            <img class="icon" src="~/images/Group 13462.svg"  />
                        </a></li>
                    }
                </div>
                <div class="log-in">
                    <li class="nav-item dropdown">
                        @if ((@Html.Action("GetUserName", "User")).ToString() != "")

                        {<a class="nav-link dropdown-toggle"
                            href="#"
                            role="button"
                            data-bs-toggle="dropdown"
                            aria-expanded="false">
                                @Html.Action("GetUserName", "User")  <img src="~/images/contacts.svg" alt="" />
                            </a>
                            <ul class="dropdown-menu bg-white">
                                <li>
                                    <a class="dropdown-item" href=" @Url.Action("Logout", "User") ">
                                        Logout
                                        <img class="ps-2" src="~/images/contacts.svg" alt="" />
                                        <i class="fa-solid fa-angle-left"></i>
                                    </a>
                                </li>

                            </ul>
                          

                        }
                        else
                        {

                            <a class="nav-link dropdown-toggle"
                               href="#"
                               role="button"
                               data-bs-toggle="dropdown"
                               aria-expanded="false">
                                log in <img src="~/images/contacts.svg" alt="" />
                            </a>
                            <ul class="dropdown-menu bg-white">

                                <li>
                                    <button id="sign-in" type="button" class="dropdown-item" data-toggle="modal" data-target="#exampleModalCenter1">
                                        log in
